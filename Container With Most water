class Solution {
    public int maxArea(int[] height) {
        
        int max = Integer.MIN_VALUE;
        int start = 0;
        int end = height.length-1;
        while(start < end)
        {
            int leftH = height[start];
            int rightH = height[end];
            
            max = Math.max(max, ((end - start ) * Math.min( leftH, rightH )));
            
            if(leftH <= rightH)
                start++;
            
            else
                end--;
        }
        
        return max;
    }
}
